//Question 5
let nameLengths = [];
for (let index = 0; index < names.length; index++){
  nameLengths[index] = names[index].length; 
}

console.log(nameLengths);

//Question 6
let totalNameLengths = 0;
for (let nameLength of nameLengths){
    totalNameLengths += nameLength;
}
console.log(totalNameLengths);

//Question 7
function concatenated(word, number){
    let concatWord = "";
    for (let i= 1; i<=number; i++){
      concatWord +- word;
    }
    return concatWord;
  }
  
//Question 8
function fullName(firstName, lastName) {
    console.log (firstName + ' ' + lastName)
}
fullName("Jaden", "LaBrosse");

//Question 9
function numberArray(array){
    let sum =0
    for (let element of array){
        sum += element;
    }
    if (sum >100){
        return true;
    }
    return false;
  }
  let sumHundred = [25, 25, 25, 26]
  console.log(numberArray(sumHundred));

//Question 10
function averageArray(x) {
let total = 0
 
for (let element of x){
     total += element;
}
return total/x.length;
}    
let x = [6, 2, 5, 4, 9, 5, 1,];
console.log(averageArray(x));


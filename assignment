//Question 1
ages = [3, 9, 23, 64, 2, 8, 28, 93];
let lastIndex = ages.length-1;
let firstIndex = 0;
ages.push (7)
console.log(ages[lastIndex]-ages[firstIndex]);

var total = 0
for (var i = 0; i < ages.length; i++) {
    total += ages[i]
}
var average = total / ages.length;

console.log(ages.length)
console.log(average)

//Question 2
let names = ['Sam ', 'Tommy ', 'Tim ', 'Sally ', 'Buck ', 'Bob'];
let namesCharTotal = 0 
for (let name of names) {
    namesCharTotal += name.length; 
}
console.log(namesCharTotal/names.length);

let contactNames = "";
for (let name of names) {
    contactNames += name; 
}

console.log(contactNames);

